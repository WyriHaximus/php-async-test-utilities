{
  "name": "wyrihaximus/async-test-utilities",
  "description": "Test utilities for api-clients packages",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com"
    }
  ],
  "require": {
    "php": "^8 || ^7.4",
    "clue/block-react": "^1.5",
    "phpunit/phpunit": "^9.5",
    "react/event-loop": "^1.2",
    "react/promise": "^2.8",
    "wyrihaximus/test-utilities": "^3.7.5"
  },
  "require-dev": {
    "wyrihaximus/iterator-or-array-to-array": "^1.2"
  },
  "config": {
    "platform": {
      "php": "7.4.7"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "WyriHaximus\\AsyncTestUtilities\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WyriHaximus\\Tests\\AsyncTestUtilities\\": "tests/"
    },
    "files": [
      "tests/bootstrap.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "composer normalize"
    ],
    "post-update-cmd": [
      "composer normalize"
    ]
  }
}
