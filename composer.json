{
  "name": "wyrihaximus/async-test-utilities",
  "description": "Test utilities for api-clients packages",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "phpunit/phpunit": "^9.6.7",
    "react/async": "^4.0",
    "react/event-loop": "^1.3.0",
    "react/promise": "^2.9.",
    "wyrihaximus/test-utilities": "^5.4.0 || ^6.1.0"
  },
  "require-dev": {
    "wyrihaximus/iterator-or-array-to-array": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "WyriHaximus\\AsyncTestUtilities\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WyriHaximus\\Tests\\AsyncTestUtilities\\": "tests/"
    },
    "files": [
      "tests/bootstrap.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true,
      "icanhazstring/composer-unused": true,
      "infection/extension-installer": true
    },
    "platform": {
      "php": "8.2.13"
    },
    "sort-packages": true
  },
  "scripts": {
    "post-install-cmd": [
      "composer normalize"
    ],
    "post-update-cmd": [
      "composer normalize"
    ]
  }
}
